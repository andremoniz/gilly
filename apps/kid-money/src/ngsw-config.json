{
	"index": "/index.html",
	"assetGroups": [
		{
			"name": "app",
			"installMode": "prefetch",
			"resources": {
				"files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
			}
		},
		{
			"name": "assets",
			"installMode": "lazy",
			"updateMode": "prefetch",
			"resources": {
				"files": ["/assets/**", "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"]
			}
		}
	],
	"dataGroups": [
		{
			"name": "api",
			"urls": ["https://kidmoney.herokuapp.com/**"],
			"cacheConfig": {
				"maxSize": 100,
				"maxAge": "3d",
				"timeout": "1m",
				"strategy": "freshness"
			}
		}
	]
}
